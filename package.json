{
    "name": "@vidsentry/sdk",
    "version": "0.1.0",
    "description": "Official VidSentry SDK for video content moderation with African cultural awareness",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
        "test": "vitest run",
        "lint": "eslint src/",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "vidsentry",
        "video",
        "moderation",
        "content-moderation",
        "ai",
        "african",
        "hate-speech",
        "safety"
    ],
    "author": "VidSentry",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/vidsentry/sdk-js"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0",
        "vitest": "^1.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}